                        <property name="name">
                            <cstring>textLabel5</cstring>
                        </property>
                        <property name="text">
                            <string>Enter a name, description and ODBC driver file. Driver files are commonly stored in /usr/lib on UNIX systems. Select an ODBC setup library if one was supplied with the driver.</string>
                        </property>
                        <property name="alignment">
                            <set>WordBreak|AlignVCenter</set>
                        </property>
                    </widget>
