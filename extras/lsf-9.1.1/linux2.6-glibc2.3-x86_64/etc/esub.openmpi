#!/bin/sh
#$RCSfile: esub.openmpi,v $Revision: 1.3 $Date: 2013/01/10 05:51:08 $

# Redirect stderr to stdout so echo can be used for error
exec 1>&2

if [ -z "$LSB_SUB_PARM_FILE" ]; then
    exit 0
fi

. $LSB_SUB_PARM_FILE

if [ "$LSB_SUB_ADDITIONAL" = "" ]; then
    exit 0
fi

echo "LSF_PJL_TYPE=\"openmpi\"" >> $LSB_SUB_MODIFY_ENVFILE

exit 0
