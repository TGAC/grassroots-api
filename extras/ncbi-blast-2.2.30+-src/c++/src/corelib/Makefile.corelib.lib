      ncbi_param ncbi_process ncbi_safe_static ncbi_signal ncbi_stack \
      ncbi_system ncbiapp ncbiargs ncbiatomic ncbicfg ncbidbg ncbidiag \
      ncbidiag_p ncbidll ncbienv ncbiexec ncbiexpt ncbifile ncbimempool \
